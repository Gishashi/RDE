{
  "name": "rde",
  "description": "Recommanded Development Environment",
  "version": "0.0.1-alpha.26",
  "author": "kaolafed",
  "bin": {
    "rde": "./bin/run"
  },
  "bugs": "https://github.com/kaola-fed/rede/issues",
  "dependencies": {
    "@oclif/command": "^1.5.12",
    "@oclif/config": "^1.12.12",
    "@oclif/plugin-help": "^2",
    "axios": "^0.18.0",
    "browser-sync": "^2.26.4",
    "chokidar": "^2.1.5",
    "cli-table2": "^0.2.0",
    "deasync": "^0.1.15",
    "debug": "^4.1.1",
    "deep-extend": "^0.6.0",
    "directory-tree": "^2.2.1",
    "enquirer": "^2.3.0",
    "fs-extra": "^7.0.1",
    "highlight.js": "^9.15.6",
    "js-beautify": "^1.9.1",
    "markdown-it": "^8.4.2",
    "markdown-it-meta": "0.0.1",
    "mustache": "^3.0.1",
    "npm-which": "^3.0.1",
    "recursive-copy": "^2.0.10",
    "rimraf": "^2.6.3",
    "semver": "^6.0.0",
    "signale": "^1.4.0",
    "staged-git-files": "^1.2.0",
    "through2": "^3.0.1",
    "ts-node": "^8.0.3",
    "typescript": "^3.3",
    "vue": "^2.6.10",
    "vue-server-renderer": "^2.6.10",
    "vue-template-compiler": "^2.6.10",
    "write-pkg": "^3.2.0"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@oclif/test": "^1",
    "@oclif/tslint": "^3",
    "@types/chai": "^4",
    "@types/deep-extend": "^0.4.31",
    "@types/mocha": "^5",
    "@types/mustache": "^0.8.32",
    "@types/node": "^10",
    "@types/signale": "^1.2.1",
    "@types/through2": "^2.0.34",
    "chai": "^4.2.0",
    "globby": "^8",
    "husky": "^1.3.1",
    "lint-staged": "^8.1.5",
    "mocha": "^5",
    "nyc": "^13",
    "sinon": "^7.3.1",
    "tslint": "^5",
    "tslint-lines-between-class-members": "^1.3.1",
    "validate-commit-msg": "^2.14.0"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/kaola-fed/rede",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "rde",
    "hooks": {
      "checkUpdate": [
        "./lib/hooks/check.update"
      ]
    },
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "git add"
    ]
  },
  "repository": "kaolafed/rde",
  "scripts": {
    "posttest": "tslint -p test -t stylish",
    "prepack": "tsc -b && oclif-dev readme && cp -r src/mustaches lib/mustaches",
    "manifest": "oclif-dev manifest",
    "test:coverage": "nyc --extension .ts mocha --project ./test/tsconfig.json --opts ./test/mocha.opts --forbid-only \"test/**/*.test.ts\"",
    "test": "mocha --project ./test/tsconfig.json --opts ./test/mocha.opts --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md",
    "commitmsg": "validate-commit-msg",
    "lint": "tsc --pretty && tslint --project ./tslint.json",
    "lint:fix": "tsc --pretty && tslint --project ./tslint.json --fix"
  },
  "types": "lib/index.d.ts"
}
